{
  "name": "kaluzki/oxid-api",
  "description": "oxid restful api",
  "keywords": ["oxid", "modules", "restful", "api"],
  "homepage": "https://github.com/kaluzki/oxid-api",
  "license": "MIT",
  "authors": [{"name": "Demjan Kaluzki"}],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6",
    "twig/twig": "^1",
    "league/flysystem": "^1",
    "php-di/slim-bridge": "^1",
    "mnapoli/silly": "^1",
    "php-fn/php-fn": "^1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "oxid-esales/oxideshop-ce": "^6",
    "oxid-esales/paypal-module": "^5",
    "oxid-esales/oxideshop-facts": "^2",
    "oxid-esales/oxideshop-db-views-generator": "^1",
    "oxid-esales/oxideshop-composer-plugin": "^2",
    "oxid-esales/oxideshop-unified-namespace-generator": "^1",
    "oxid-esales/oxideshop-demodata-ce": "^6"
  },
  "autoload": {
    "psr-4": {"kaluzki\\": ["src"]},
    "files": ["src/autoload-files.php"]
  },
  "autoload-dev": {
    "psr-4": {"kaluzki\\": ["tests"]}
  },
  "bin": [
    "bin/oxid-api"
  ]
}
